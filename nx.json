{
  "npmScope": "nx-monolith-layers",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "composition-users": {
      "tags": [
        "type:composition"
      ]
    },
    "shell": {
      "tags": [
        "type:app"
      ]
    },
    "shell-e2e": {
      "tags": [],
      "implicitDependencies": [
        "shell"
      ]
    }
  }
}